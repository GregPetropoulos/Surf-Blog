{
  "kind": "collectionType",
  "collectionName": "participants",
  "info": {
    "singularName": "participant",
    "pluralName": "participants",
    "displayName": "Participant",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true,
      "minLength": 5,
      "maxLength": 25,
      "regex": "^[\\+]?[(]?[0-9]{3}[)]?[-\\s\\.]?[0-9]{3}[-\\s\\.]?[0-9]{4,6}$",
      "unique": false
    },
    "isGeneralInterest": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "event": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event.event",
      "inversedBy": "participants"
    }
  }
}
